<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.refcodes.portlets.businesscenter">
	<namespace>businesscenter</namespace>
	<entity name="BusinessUser" uuid="true" local-service="true" remote-service="false">
		<column name="businessUserId" type="long" primary="true" />
		<column name="groupId" type="long" />
		<column name="companyId" type="long" />
		<column name="firstName" type="String" />
		<column name="lastName" type="String" />
		<column name="email" type="String" />
		<column name="ownerShip" type="String" />
		<column name="categoryId" type="long" />
		<column name="phoneNumber" type="String" />
		<column name="createdDate" type="Date" />
		<column name="createdBy" type="long" />
		<column name="updatedDate" type="Date" />
		<column name="updatedBy" type="long" />
	</entity>
</service-builder>