<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.refcodes.portlets.businesscenter.model.BusinessUser" />
	<class name="com.refcodes.portlets.businesscenter.model.impl.BusinessUserImpl" table="businesscenter_BusinessUser">
		<cache usage="read-write" />
		<id name="businessUserId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="firstName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="lastName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="ownerShip" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="categoryId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="phoneNumber" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createdDate" type="org.hibernate.type.TimestampType" />
		<property name="createdBy" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="updatedDate" type="org.hibernate.type.TimestampType" />
		<property name="updatedBy" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>